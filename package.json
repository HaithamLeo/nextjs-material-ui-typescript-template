{
  "name": "nextjs-material-ui-app",
  "version": "1.0.0",
  "description": "Simple project that shows the usage of Next.js and Material-ui React components with TypeScript",
  "keywords": [
    "react",
    "next.js",
    "material UI",
    "typescript",
    "javascript"
  ],
  "main": "next.config.js",
  "author": "Haitham Leo <alkhomayasi3@gmail.com>",
  "license": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaithamLeo/nextjs-material-ui-typescript-template.git"
  },
  "bugs": {
    "url": "https://github.com/HaithamLeo/nextjs-material-ui-typescript-template/issues"
  },
  "homepage": "https://github.com/HaithamLeo/nextjs-material-ui-typescript-template#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/next": "^9.0.0",
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@zeit/next-typescript": "^1.1.1",
    "next": "^10.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "babel-plugin-styled-components": "^1.12.0",
    "typescript": "^4.2.3"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
